<header>
  <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
    <div class="container-fluid">
      <a class="navbar-brand" href="#">TinyA🅿️🅿️</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item">
            <a class="nav-link" href="/urls">My URLs</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/urls/new">New URL</a>
          </li>
        </ul>
      </div>
      <% if(users[user_id]){ %>
        <h4 class="text mx-2 text-light mt-2"><%= users[user_id].email %></h4>
        <form class="d-flex mx-2" action="/logout" method="POST">
          <button class="btn btn-outline-light" type="submit">Logout</button>
        </form>
      <% } else {%>
        <form class="d-flex mx-2" action="/login" method="GET">
          <button class="btn btn-outline-light" type="submit">Login</button>
        </form>
        <form class="d-flex mx-2"  action="/register" method="GET">
          <button class="btn btn-outline-light" type="submit">Register</button>
        </form>
      <% } %>
    </div>
  </nav>
</header>